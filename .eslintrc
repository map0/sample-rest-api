{
    "extends": "airbnb",
    "env": {
      "node": true,
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "script",
      "ecmaFeatures": {
        "modules": false
      }
    },
    "rules": {
      "comma-dangle": [0],
      "max-len": [0],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "generator-star-spacing": 0,
      "semi": [0],
      "arrow-parens": [0],
      "func-names": [0],
      "consistent-return": [1],
      "no-unused-vars": ["error", { "argsIgnorePattern":"^next"}]
    }
}
